@model MVC.Models.LoginViewModel
@{
    ViewBag.Title = "Login";
}

<h2>Login</h2>

@{
    using (Html.BeginForm()) 
    { 
        @Html.Label("Name", "Name");
        @Html.TextBox("Name");
        @Html.Label("Password", "Password");
        @Html.Password("Password");
        <input type="submit" value="Login" />
    }
    
}

